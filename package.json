{
  "name": "Tweet Sentiment Analyzer",
  "version": "1.0.0",
  "description": "MERN stack application that allows user to enter their twitter handle and see their overall sentiment",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "client": "npm run start --prefix client",
    "heroku-postbuild": "cd client/  && npm install && npm install --only=dev -no-shrinkwrap && npm run build",  

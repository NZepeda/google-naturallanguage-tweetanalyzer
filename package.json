  "license": "ISC",
  "dependencies": {
    "@google-cloud/language": "0.12.1",
    "body-parser": "1.18.2",
    "concurrently": "3.5.1",
    "dotenv": "4.0.0",
    "express": "4.16.1",
    "google-proto-files": "0.13.1",
    "googleapis": "22.2.0",
    "lodash": "4.17.4",
    "mongoose": "4.12.0",
    "newrelic": "2.2.2",
    "node-schedule": "1.2.5",
    "react": "16.0.0",
    "twitter": "1.7.1"
  },
  "engines": {
    "node": "8.9.1",
    "npm": "5.5.1"
  }
